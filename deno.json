{
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "useUnknownInCatchVariables": true
  },
  "fmt": {
    "files": { "exclude": ["dist"] },
    "options": { "semiColons": false, "singleQuote": true }
  },
  "imports": {
    "@/ooz": "./mod.ts",
    "dejs/": "https://deno.land/x/dejs@0.10.3/",
    "std/": "https://deno.land/std@0.179.0/"
  },
  "lint": {
    "files": { "exclude": ["dist"] },
    "rules": {
      "exclude": [
        "no-empty-interface",
        "no-inferrable-types",
        "no-misused-new",
        "no-namespace"
      ]
    }
  },
  "lock": false,
  "name": "ooz",
  "test": { "files": { "exclude": ["dist"] } },
  "version": "0.0.15"
}
