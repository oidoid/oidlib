{
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "useUnknownInCatchVariables": true
  },
  "fmt": {
    "files": { "exclude": ["dist"] },
    "options": { "semiColons": false, "singleQuote": true }
  },
  "imports": { "@/ooz": "./mod.ts", "std/": "https://deno.land/std@0.179.0/" },
  "lint": {
    "files": { "exclude": ["dist"] },
    "rules": { "exclude": ["no-empty-interface", "no-misused-new"] }
  },
  "lock": false,
  "name": "ooz",
  "test": { "files": { "exclude": ["dist"] } },
  "version": "0.0.17"
}
